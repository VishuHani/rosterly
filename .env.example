# =============================================
# Rosterly Environment Variables Template
# =============================================
# Copy this file to .env and fill in your values
# NEVER commit the .env file to git!

# =============================================
# SUPABASE
# =============================================
# Get these from: https://supabase.com/dashboard/project/_/settings/api

SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-public-key-here
SUPABASE_SERVICE_KEY=your-service-role-key-here

# =============================================
# OPENAI
# =============================================
# Get this from: https://platform.openai.com/api-keys

OPENAI_API_KEY=sk-your-openai-api-key-here

# =============================================
# VERCEL API
# =============================================
# This is your deployed Vercel API URL
# Format: https://your-project.vercel.app/api

API_BASE_URL=https://your-project.vercel.app/api

# =============================================
# FIREBASE (Push Notifications)
# =============================================
# Get this from: Firebase Console → Project Settings → Cloud Messaging

FCM_SERVER_KEY=your-firebase-server-key-here

# =============================================
# CONFIGURATION
# =============================================

# Name matching similarity threshold (0.0 to 1.0)
# Higher = stricter matching. Default: 0.83
VECTOR_SIM_THRESHOLD=0.83

# Log level (debug, info, warning, error)
LOG_LEVEL=info

# Environment (development, staging, production)
NODE_ENV=development

# =============================================
# OPTIONAL: FUTURE INTEGRATIONS
# =============================================

# OneSignal (Alternative to FCM)
# ONESIGNAL_APP_ID=your-onesignal-app-id
# ONESIGNAL_REST_API_KEY=your-onesignal-rest-api-key

# Xero (Payroll integration)
# XERO_CLIENT_ID=your-xero-client-id
# XERO_CLIENT_SECRET=your-xero-client-secret

# MYOB (Payroll integration)
# MYOB_CLIENT_ID=your-myob-client-id
# MYOB_CLIENT_SECRET=your-myob-client-secret

# =============================================
# NOTES
# =============================================
#
# 1. Keep this file secure - it contains sensitive credentials
# 2. Use different values for development and production
# 3. In Vercel, add these as Environment Variables in the dashboard
# 4. For Flutter, update lib/config/app_config.dart with your values
# 5. Test each integration separately before going live
